@{
    ViewData["Title"] = "Import";
}

<form method="post" asp-action="Import" asp-controller="Test" enctype="multipart/form-data" style="width:50%;">
    <input type="file" name="importFile" class="form-control" required />
    <select name="fileType" class="form-control">
        <option value="xlsx">
            XSLX
        </option>
        <option value="csv">
            CSV
        </option>
    </select>
    <button type="submit" class="btn btn-primary">
        Import
    </button>
</form>